apt update
apt install -y zstd
cd
mkdir -v a
cd a
wget http://raw.githubusercontent.com/CE1CECL/n/main/repo
cp -vrf ./repo /usr/bin/repo
chmod 7777 -vR /usr/bin/repo
export HTAP=$PWD/Download/
mkdir -vp $HTAP
cd $HTAP
rm -vrf ./*
rm -vrf ./.*
echo "maybe so" > yes.no
mkdir -vp main
cd main
repo init -u http://github.com/CE1CECL/n -c --no-tags --dissociate --partial-clone --no-use-superproject --no-clone-bundle --no-git-lfs --no-repo-verify -m main.xml
repo sync -j$(nproc --all) --fail-fast --nmu -d -c --no-clone-bundle --no-use-superproject --no-tags --prune --no-repo-verify
rm -vrf ./.repo
cd $HTAP/main/
rm -vrf $HTAP/yes.no
tar -I 'zstd --ultra -22 -v -T0 -r --format=zstd' -c -v -f '/home/runner/main.tar.zst' './'
chmod 7777 -vR /home/runner/main.tar.zst
